{
    "name": "ahmed/design-pattern-in-php",
    "description": "code example for design patterns in php",
    "minimum-stability": "dev",
    "authors": [
        {
            "name": "ahmedabdellatifss",
            "email": "ahmedabdellatifss@yahoo.com"
        }
    ],
    "require": {
        "php": "^7.3|^8.0",
        "phpunit/phpunit": "^8.5",
        "ahmed/basic-auth": "dev-master",
        "ahmed/token-auth-lib": "dev-master"
    },

    "autoload": {
        "psr-4": {
            "Creational\\": "Creational",
            "Structural\\": "Structural",
            "Behavioral\\": "Behavioral",
            "Repositories\\": "Repositories",
            "Others\\": "Others"
        }
    },

    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "repositories": [
        {
            "type": "path",
            "url": "BasicAuth"
        },
        {
            "type": "path",
            "url": "TokenAuthLib"
        }
    ],
    "license": ["MIT"]
}
